<h2 class="page-header">Gallery</h2>
<p>Welcome</p>  

    <div class="row">
    <div class="card card-body col-md-12">

      {{#each images as |image|}}
          <div id="{{image.id}}" class="image">
            <div class="delete">Delete</div><img src="{{images.image}}">
          </div>
        {{/each}}
    </div>


      <ul class="thumbnails">
      {{#each elements}}
        <li href="/gallery" class="card card-body col-md-6"><a class="thumbnail" href="{{permalink}}" target="_blank"><img src="{{image.image}}" title="{{data.image.caption}}" /></a></li>
      {{/each}}
      </ul>
    </div>
    </div>
<br>